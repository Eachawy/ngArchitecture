{
    "name": "svis",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "./node_modules/.bin/nx lint && ng lint",
        "e2e": "ng e2e",
        "affected:apps": "./node_modules/.bin/nx affected:apps",
        "affected:libs": "./node_modules/.bin/nx affected:libs",
        "affected:build": "./node_modules/.bin/nx affected:build",
        "affected:e2e": "./node_modules/.bin/nx affected:e2e",
        "affected:test": "./node_modules/.bin/nx affected:test",
        "affected:lint": "./node_modules/.bin/nx affected:lint",
        "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
        "affected": "./node_modules/.bin/nx affected",
        "format": "./node_modules/.bin/nx format:write",
        "format:write": "./node_modules/.bin/nx format:write",
        "format:check": "./node_modules/.bin/nx format:check",
        "update": "ng update @nrwl/schematics",
        "update:check": "ng update",
        "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
        "dep-graph": "./node_modules/.bin/nx dep-graph",
        "help": "./node_modules/.bin/nx help",
        "build:inspection": "ng build --prod --project inspection --single-bundle true --output-hashing all --vendor-chunk false",
        "build:inspection-copy": "rm -rf apps/shell/src/assets/app/inspection && md \"apps/shell/src/assets/app/inspection\" && cp -a dist/apps/inspection/assets/app/inspection/.  apps/shell/src/assets/app/inspection && rm -r apps/shell/src/assets/app/inspection/main.*.js && cp dist/apps/inspection/main*.js apps/shell/src/assets/app/inspection/",
        "inspection-post-build": "node ./apps/inspection/build/builder.js && tar -cvf - apps/shell/src/assets/app/inspection > dist/inspection.zip",
        "inspection:publish": "npm run build:inspection && npm run build:inspection-copy && npm run inspection-post-build",
        "build:appointment": "ng build --prod --project appointment --single-bundle true --output-hashing none --vendor-chunk false",
        "build:appointment-copy": "cat dist/apps/appointment/main.js > apps/shell/src/assets/app/appointment/main.js",
        "build:examination": "ng build --prod --project examination --single-bundle true --output-hashing none --vendor-chunk false",
        "build:examination-copy": "cat dist/apps/examination/main.js > apps/shell/src/assets/app/examination/main.js",
        "build:shell": "ng build --prod --project shell --output-hashing all --vendor-chunk false",
        "publish": "npm run inspection:publish && npm run build:shell",
        "post-build": "node ./build/post-build.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^7.1.4",
        "@angular/cdk": "^7.3.3",
        "@angular/common": "^7.0.0",
        "@angular/compiler": "^7.0.0",
        "@angular/core": "^7.0.0",
        "@angular/elements": "^7.1.1",
        "@angular/forms": "^7.0.0",
        "@angular/platform-browser": "^7.0.0",
        "@angular/platform-browser-dynamic": "^7.0.0",
        "@angular/router": "^7.0.0",
        "@ngrx/effects": "6.1.0",
        "@ngrx/router-store": "6.1.0",
        "@ngrx/store": "6.1.0",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "@nrwl/nx": "7.1.0",
        "@webcomponents/custom-elements": "^1.2.1",
        "angular2-materialize": "^15.1.10",
        "canvasjs": "^1.8.1",
        "cheerio": "^1.0.0-rc.2",
        "core-js": "^2.5.4",
        "font-awesome": "^4.7.0",
        "hammerjs": "^2.0.8",
        "jquery": "^2.2.4",
        "materialize-css": "^0.100.2",
        "ng2-dragula": "^2.1.1",
        "ngx-translate-multi-http-loader": "^2.1.2",
        "ngx-webstorage": "^3.0.0-beta.14",
        "primeicons": "^1.0.0",
        "primeng": "^7.0.3",
        "rxjs": "~6.3.3",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.13.5",
        "@angular/cli": "7.0.1",
        "@angular/compiler-cli": "^7.0.0",
        "@angular/language-service": "^7.0.0",
        "@ngrx/store-devtools": "6.1.0",
        "@nrwl/schematics": "7.1.0",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-marbles": "0.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ngrx-store-freeze": "0.2.4",
        "ngx-build-plus": "^7.2.3",
        "prettier": "1.15.2",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.1.3"
    }
}
